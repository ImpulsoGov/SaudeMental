{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": "error",
    "quotes": [
      "error",
      "single"
    ],
    "no-trailing-spaces": "error",
    "indent": [
      "error",
      2
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ]
  }
}
